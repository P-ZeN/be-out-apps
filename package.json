{
  "name": "be-out-app",
  "version": "1.0.0",
  "description": "Be-Out App Monorepo",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=client\" \"npm run dev --workspace=server\" \"npm run dev --workspace=admin-client\"",
    "dev:client": "npm run dev --workspace=client",
    "dev:server": "npm run dev --workspace=server",
    "dev:admin": "npm run dev --workspace=admin-client",
    "build": "npm run build --workspace=client && npm run build --workspace=server && npm run build --workspace=admin-client",
    "build:client": "npm run build --workspace=client",
    "build:server": "npm run build --workspace=server",
    "build:admin": "npm run build --workspace=admin-client",
    "tauri": "npm run tauri --workspace=client"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "workspaces": [
    "client",
    "server",
    "admin-client"
  ]
}
