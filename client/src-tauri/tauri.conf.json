        "longDescription": "Discover amazing events happening around you. Connect with your community and never miss out on experiences that matter.",
        "resources": [],
        "shortDescription": "Discover events in your community",
        "targets": "all",
        "iOS": {
            "minimumSystemVersion": "13.0"
        },
        "android": {
            "minSdkVersion": 24
        }
    },
    "plugins": {
        "shell": {
            "open": true
        },
        "deep-link": {
            "schemes": [
                "beout"
            ]
        },
        "google-signin": {
            "clientId": {
                "android": "1064619689471-7lr8e71tr6h55as83o8gn4bdnhabavpu.apps.googleusercontent.com",
                "desktop": "1064619689471-mrna5dje1h4ojt62d9ckmqi3e8q07sjc.apps.googleusercontent.com"
            }
