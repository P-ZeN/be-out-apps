{
    "productName": "Be Out!",
    "version": "1.0.0",
    "identifier": "com.beout.app",
    "build": {
        "beforeBuildCommand": "npm run build:client",
        "beforeDevCommand": "npm run dev",
        "frontendDist": "../build",
        "devUrl": "http://localhost:5173"
    },
    "app": {
        "windows": [
            {
                "fullscreen": false,
                "height": 600,
                "resizable": true,
                "title": "Be Out!",
                "width": 800
            }
        ],
        "withGlobalTauri": false,
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "category": "Lifestyle",
        "copyright": "Copyright Â© 2025 Be Out!",
        "icon": [
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/32x32.png",
            "icons/64x64.png",
            "icons/icon.icns",
            "icons/icon.ico",
            "icons/icon.png",
            "icons/be-out_icon_512x512.png",
            "icons/android-icon-48.png",
            "icons/android-icon-72.png",
            "icons/android-icon-96.png",
            "icons/android-icon-144.png",
            "icons/android-icon-192.png",
            "icons/Square107x107Logo.png",
            "icons/Square142x142Logo.png",
            "icons/Square150x150Logo.png",
            "icons/Square284x284Logo.png",
            "icons/Square30x30Logo.png",
            "icons/Square310x310Logo.png",
            "icons/Square44x44Logo.png",
            "icons/Square71x71Logo.png",
            "icons/Square89x89Logo.png",
            "icons/StoreLogo.png"
