{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "BeOut",
  "version": "1.0.1",
  "identifier": "com.beout.mobile",
  "build": {
    "frontendDist": "../client/build",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev:client",
    "beforeBuildCommand": "npm run build:client"
  },
  "app": {
    "windows": [
      {
        "title": "BeOut!",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "shortDescription": "BeOut - Event Discovery Platform",
    "longDescription": "BeOut is a comprehensive event discovery and management platform that connects event organizers with attendees.",
    "category": "Social Networking",
    "copyright": "Â© 2025 BeOut"
  },
  "plugins": {}
}
